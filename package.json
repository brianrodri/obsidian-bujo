{
    "name": "obsidian-bujo",
    "version": "0.0.1",
    "description": "Bring the Bullet Journal method to your Obsidian vault.",
    "author": "Brian Rodriguez <brian@brianrodri.com>",
    "license": "MIT",
    "keywords": [
        "obsidian",
        "bullet journal"
    ],
    "main": "main.js",
    "scripts": {
        "prepare": "husky install",
        "test": "jest --coverage",
        "dev": "rollup --config rollup.config.js -w",
        "build": "BUILD=PROD rollup --config rollup.config.js",
        "watch": "jest --coverage --watchAll",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "lint": "eslint --fix --max-warnings=0 .",
        "format": "prettier --cache --write .",
        "generate-docs": "typedoc"
    },
    "lint-staged": {
        "**/*": "eslint --fix --max-warnings=0 ",
        "**/*": "prettier --cache --write --ignore-unknown "
    },
    "dependencies": {
        "luxon": "latest",
        "obsidian-dataview": "0.5.56",
        "react": "latest",
        "react-dom": "latest"
    },
    "devDependencies": {
        "@jest/globals": "latest",
        "@rollup/plugin-commonjs": "latest",
        "@rollup/plugin-eslint": "latest",
        "@rollup/plugin-node-resolve": "latest",
        "@rollup/plugin-typescript": "latest",
        "@types/luxon": "latest",
        "@types/node": "latest",
        "@types/react": "latest",
        "@types/react-dom": "latest",
        "@types/rollup": "latest",
        "@types/ungap__structured-clone": "latest",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "@ungap/structured-clone": "latest",
        "builtin-modules": "latest",
        "eslint": "latest",
        "eslint-config-prettier": "latest",
        "husky": "latest",
        "jest": "latest",
        "jest-environment-jsdom": "latest",
        "lint-staged": "latest",
        "obsidian": "latest",
        "prettier": "latest",
        "rollup": "latest",
        "rollup-plugin-copy": "latest",
        "rollup-plugin-typescript2": "latest",
        "rollup-plugin-web-worker-loader": "latest",
        "ts-jest": "latest",
        "ts-node": "latest",
        "tslib": "latest",
        "typedoc": "latest",
        "typedoc-plugin-coverage": "latest",
        "typescript": "latest"
    },
    "//": "TODO(liamcain/obsidian-calendar-ui#20): Needed to get off of a vulnerable svelte version",
    "overrides": {
        "svelte": "^3.59.2"
    }
}
